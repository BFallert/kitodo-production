<?xml version="1.0" encoding="UTF-8"?>
<tns:properties xmlns:tns="http://www.goobi.org/propertyTemplates" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.goobi.org/propertyTemplates propertyTemplates.xsd ">


	<!--  scannen -->

	<tns:property name="Öffnungswinkel" required="true" container="1">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="project" />
		<tns:entity type="step" name="Scannen" />
		<tns:type>List</tns:type>
		<tns:value value="90°" />
		<tns:value value="180°" />
	</tns:property>


	<tns:property name="Scangerät" required="true" container="1">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="step" name="Scannen" />
		<tns:type>ListMultiSelect</tns:type>
		<tns:value value="Minolta 3000" />
		<tns:value value="Minolta 7000" />
		<tns:value value="Zeutschel 7000" />
		<tns:value value="Zeutschel 3000" />
		<tns:value value="Farbe-Martin" />
		<tns:value value="Farbe AGFA" />
		<tns:value value="I2S 6002" />
		<tns:value value="Zeutschel 10000" />
		<tns:value value="ScanRobot" />
		<tns:value value="WF BuchSpiegel" />
		<tns:value value="Traveller" />
	</tns:property>


	<!-- Qualitätskontrolle -->

	<tns:property name="Druckqualität" required="true">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="project" />
		<tns:entity type="step" name="Qualitätskontrolle" />
		<tns:type>List</tns:type>
		<tns:value value="gut" />
		<tns:value value="geht" />
		<tns:value value="schlecht" />
		<tns:value value="nicht druckbar" />
	</tns:property>


	<!-- Imaging -->

	<tns:property name="Anzahl Farbe" required="true">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="step" name="Imagenachbearbeitung" />
		<tns:type>Number</tns:type>
	</tns:property>

	<tns:property name="Anzahl Grau" required="true">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="step" name="Imagenachbearbeitung" />
		<tns:type>Number</tns:type>
	</tns:property>

	<tns:property name="Anzahl Bitonal" required="true">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="step" name="Imagenachbearbeitung" />
		<tns:type>Number</tns:type>
	</tns:property>

	<!-- PDF Erstellung/Digiwunschbuch -->
	
	<tns:property name="Shiped">
		<tns:entity type="project" name="DigiWunschBuch" />
		<tns:entity type="process" />
		<tns:entity type="step" name="PDF Erstellung" />
		<tns:type>Boolean</tns:type>
	</tns:property>

	<tns:property name="PdfCreated">
		<tns:entity type="project" name="DigiWunschBuch" />
		<tns:entity type="process" />
		<tns:entity type="step" name="PDF Erstellung" />
		<tns:type>Boolean</tns:type>
	</tns:property>



	<!-- each process -->



	<!-- processes on project DigiWunschbuch -->

	<tns:property name="Sponsor">
		<tns:entity type="project" name="DigiWunschBuch" />
		<tns:entity type="process" />
		<tns:type>Number</tns:type>
	</tns:property>

	<tns:property name="SponsorNaming">
		<tns:entity type="project" name="DigiWunschBuch" />
		<tns:entity type="process" />
		<tns:type>Number</tns:type>
	</tns:property>


	<tns:property name="SponsorName">
		<tns:entity type="project" name="DigiWunschBuch" />
		<tns:entity type="process" />
		<tns:type>String</tns:type>
	</tns:property>

	<tns:property name="StempUsed">
		<tns:entity type="project" name="DigiWunschBuch" />
		<tns:entity type="process" />
		<tns:type>Number</tns:type>
	</tns:property>


	<!-- Step Archivierung -->

	<tns:property name="CdSave">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="step" name="Archivierung" />
		<tns:type>String</tns:type>
	</tns:property>

	<tns:property name="MaArchiv">
		<tns:entity type="project" />
		<tns:entity type="process" />
		<tns:entity type="step" name="Archivierung" />
		<tns:type>String</tns:type>
	</tns:property>

</tns:properties>
