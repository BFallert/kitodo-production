<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.sub.goobi.beans.Werkstueck" table="werkstuecke">
		<id name="id" column="WerkstueckeID" type="integer">
			<generator class="native"/>
		</id>
		
		<many-to-one name="prozess" class="de.sub.goobi.beans.Prozess" column="ProzesseID" foreign-key="FK_ProzesseID"/>
		
		<set name="eigenschaften" cascade="all,delete-orphan" inverse="true"  order-by="titel">
			<key column="werkstueckeID"/>
			<one-to-many class="de.sub.goobi.beans.Werkstueckeigenschaft"/>
		</set>
		
	</class>

</hibernate-mapping>
