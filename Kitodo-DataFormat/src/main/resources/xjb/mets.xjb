<jxb:bindings xmlns="http://java.sun.com/xml/ns/jaxb"
              xmlns:xsd="http://www.w3.org/2001/XMLSchema"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              version="2.1">
    <bindings schemaLocation="../xsd/mets.xsd">
        <schemaBindings>
            <package name="org.kitodo.metsmodskitodo"/>
        </schemaBindings>
        <bindings node="//xsd:complexType[@name='divType']">
            <bindings node="//xsd:attribute[@ref='xlink:label']">
                <property name="xlinkLabel"/>
            </bindings>
        </bindings>
    </bindings>
</jxb:bindings>
